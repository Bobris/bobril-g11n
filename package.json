{
    "name": "bobril-g11n",
    "version": "5.2.0",
    "description": "Bobril globalization",
    "main": "index.js",
    "typescript": {
        "main": "index.ts"
    },
    "dependencies": {
        "bobril": "*",
        "moment": "^2.10.6"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120
    },
    "bobril": {
        "compilerOptions": {
            "declaration": true,
            "noImplicitAny": true,
            "noImplicitThis": true,
            "noUnusedLocals": true,
            "noUnusedParameters": true,
            "noImplicitReturns": true,
            "noFallthroughCasesInSwitch": true,
            "noUncheckedIndexedAccess": true,
            "noPropertyAccessFromIndexSignature": true,
            "strict": true,
            "strictNullChecks": true
        }
    },
